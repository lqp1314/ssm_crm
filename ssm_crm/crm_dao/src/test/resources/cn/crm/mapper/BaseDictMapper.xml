<mapper namespace="cn.crm.mapper.BaseDictMapper">
  
  
  <!-- 根据类别代码查询数据  -->
  <select id="queryBaseDictByDictTypeCode" parameterType="String" resultType="cn.crm.pojo.BaseDict">
